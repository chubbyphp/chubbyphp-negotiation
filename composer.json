    "require": {
        "php": "~7.0",
        "psr/http-message": "~1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15.1",
        "phploc/phploc": "^4.0.1|^5.0",
        "phpstan/phpstan": "^0.9.2",
        "phpunit/phpunit": "^6.5.14|^7.5.14|^8.2.3",
        "pimple/pimple": "~3.0"
    },
    "autoload": {
        "psr-4": { "Chubbyphp\\Negotiation\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Chubbyphp\\Tests\\Negotiation\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    }
