{
    "name": "chubbyphp/chubbyphp-negotiation",
    "description": "Chubbyphp Negotiation",
    "keywords": ["chubbyphp", "negotiation"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dominik Zogg",
            "email": "dominik.zogg@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "chubbyphp/chubbyphp-mock": "^1.4",
        "friendsofphp/php-cs-fixer": "^2.15.3",
        "infection/infection": "^0.14.1",
        "nunomaduro/phpinsights": "^1.9",
        "phploc/phploc": "^5.0",
        "phpstan/phpstan": "^0.11.16",
        "phpunit/phpunit": "^8.4",
        "pimple/pimple": "^3.2.3"
    },
    "autoload": {
        "psr-4": { "Chubbyphp\\Negotiation\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Chubbyphp\\Tests\\Negotiation\\": "tests/" }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    }
}
